var LBS = $feature.AmountUsed
var Product = DomainName($feature,"Product")
if(isEmpty($feature.Acres) && datediff(now(), $feature.Date, 'days')<7){
    var status = "sample";
}
/// New Labels
if($feature.Product == 'Natular G (EPA#8329-80)' && (datediff(now(), $feature.Date, 'days'))<7){
var status = LBS + ' LBS of ' + Product;
}
else if($feature.Product == 'Vectobac GR (EPA#73049-486)' && (datediff(now(), $feature.Date, 'days'))<7){
var status = LBS + ' LBS of ' + Product;
}
